{
  "name": "battle-rolly",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@koa/router": "^8.0.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/koa": "^2.11.0",
    "@types/koa__router": "^8.0.2",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "koa": "^2.11.0",
    "nodemon": "^2.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "ts-node": "^8.6.2",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "client:start": "react-scripts start",
    "client:build": "react-scripts build",
    "client:test": "react-scripts test",
    "server:start": "nodemon --watch \"src/**/*.ts\", --exec \"ts-node\" --project tsconfig.server.json src/server/server.ts",
    "server:build": "tsc --project tsconfig.server.json",
    "server:test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run client:build && npm run server:build",
    "start": "node build/server.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
