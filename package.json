  "scripts": {
    "client:start": "react-scripts start",
    "client:build": "react-scripts build && react-snap && workbox generateSW workbox-config.js",
    "client:clean": "rimraf build",
    "client:test": "react-scripts test",
    "server:watch": "yarn run server:clean && tsc --watch --project tsconfig.server.json",
    "server:start": "nodemon --inspect=3002",
    "server:build": "yarn run server:clean && tsc --project tsconfig.server.json",
    "server:clean": "rimraf build-server",
    "server:test": "jest",
    "create-migration": "yarn run server:build && node build-server/db-tools/create-migration.js",
    "apply-migrations": "yarn run server:build && node build-server/db-tools/apply-migrations.js",
    "clean": "yarn run server:clean && yarn run client:clean ",
    "test": "yarn run server:test && yarn run client:test",
    "pre-deploy": "yarn run server:build && yarn run server:start",
    "deploy": "yarn run client:build && gcloud app deploy -q && yarn run clean",
    "start": "node build-server/server.js"
  },
